                : monthname == "March" ? "03"
                    : monthname == "April" ? "04"
                        : monthname == "May" ? "05"
                            : monthname == "June" ? "06"
                                : monthname == "July" ? "07"
                                    : monthname == "August" ? "08"
                                        : monthname == "September" ? "09"
                                            : monthname == "October" ? "10"
                                                : monthname == "November" ? "11"
                                                    : monthname == "December" ? "12" : null;
        var day = $('#datetimepicker3').val().split(" ")[1].split(",")[0];

        var hour = $('#ampmpicker').val() == "PM" && parseInt($('#hourpicker').val()) < 12 ? (parseInt($('#hourpicker').val()) + 12).toString() : $('#ampmpicker').val() == "AM" && $('#hourpicker').val() == "12" ? "00" : $('#hourpicker').val();
        var meetingDateTime = $('#datetimepicker3').val().split(" ")[2] + month + day + "T" + hour + $('#minutepicker').val().toString() + "00";
        var endTime = $('#datetimepicker3').val().split(" ")[2] + month + $('#datetimepicker3').val().split(" ")[1].split(",")[0] + "T235959";
        var singleusers = $('#users').find('.singleuser.active');
        $.each(singleusers, function (k) {
            if (singleusers.eq(k).find('#firstname').val().toString() != "" && singleusers.eq(k).find('#lastname').val().toString() != "") {
                setTimeout(function () {

                    var vEmail = singleusers.eq(k).find('#visitoremail').val().toString() != "" && singleusers.eq(k).find('#visitoremail').val().toString().includes("@("@")") && singleusers.eq(k).find('#visitoremail').val().toString().includes(".") ? singleusers.eq(k).find('#visitoremail').val().toString() : "no-email@phrma.org";
                    var notify = $('#notify').val().toString();
                    if (window["recurringinterval"] == "None") {
                        $.ajax({
                            url: '../Kastle/NewVisitors',
                            contentType: 'application/json',
                            dataType: 'json',
                            type: 'POST',
                            data: JSON.stringify({
                                "GroupId": GroupId.toString(),
                                "MeetingDate": meetingDateTime,
                                "Subject": $('#subject').val().toString(),
                                "Host": $('#host').val().toString(),
                                "NotifyEmail": notify,
                                "VisitorFirstName": singleusers.eq(k).find('#firstname').val().toString(),
                                "VisitorLastName": singleusers.eq(k).find('#lastname').val().toString().split(',')[0],
                                "VisitorEmail": vEmail,
                                "VisitorOrganization": singleusers.eq(k).find('#visitororganization').val().toString(),
                                "EndDate": endTime,
                                "NiceMeetingDate": $('#datetimepicker3').val().toString(),
                                "NiceMeetingTime": $('#hourpicker').val().toString() + ":" + $('#minutepicker').val().toString() + " " + $('#ampmpicker').val().toString(),
                                "RecurringInterval": window["recurringinterval"],
                                "RecurringId": window["RecurringId"].toString()
                            })
                        });
                    } else {
                        $.ajax({
                            url: '../Kastle/NewRecurringMeeting',
                            contentType: 'application/json',
                            dataType: 'json',
                            type: 'POST',
                            data: JSON.stringify({
                                https://prod-58.eastus.logic.azure.com:443/workflows/810884e1ddb14a439a638c9fd13ccd21/triggers/manual/paths/invoke?api-version=2016-10-01&sp=%2Ftriggers%2Fmanual%2Frun&sv=1.0&sig=Jv10Q3ApUFhkES4HPhqQ0Bec5Wf7wBnRVAtK6rn6UlM
                            })
                        });
                    }

                }, 200);
            }
            if (singleusers.length - 1 == k) {
                $('#phrmaload').show();
                $('.hidephrma').hide();
                Swal.fire({
                    title: 'Visitors Added!',
                    type: 'success',
                    confirmButtonColor: '#3085d6',
                    cancelButtonColor: '#d33',
                    confirmButtonText: 'OK'

                }).then((result) => {
                    if (result.value) {
                        setTimeout(function () {
                            window.location.href = "../Kastle/MyVisitorLog";
                        }, 4000);
                    }

                });

            }
        });
    });
</script>
