<script>
        $(document).ready(function () {


            new Picker(document.querySelector('#datetimepicker3'), {
                headers: true,
                format: 'MMMM DD, YYYY',
                text: {
                    title: 'Pick a Date and Time',
                    year: 'Year',
                    month: 'Month',
                    day: 'Day'
                }
            });
            $('#host').on('select2:close', function (e) {
                var isBlank = $('#host').val() == "" ? true : false;;
                if (isBlank) {
                    $('#select2-host-container').css('border', '1px solid #dc3545');
                    $('#host-required').show();
                } else if (!isBlank) {
                    $('#select2-host-container').css('border', '1px solid green');
                    $('#host-required').hide();
                }
            });
            $('#subject').change(function (e) {
                var isBlank = $('#subject').val() == "" ? true : false;;
                if (isBlank) {
                    $('#subject').css('border', '1px solid #dc3545');
                    $('#subject-required').show();
                } else if (!isBlank) {
                    $('#subject').css('border', '1px solid green');
                    $('#subject-required').hide();
                }
            });


                $('#datetimepicker3, #hourpicker, #minutepicker, #ampmpicker').change(function (e) {
                var monthname = $('#datetimepicker3').val().split(" ")[0];
                var month = monthname == "January" ? "01"
                    : monthname == "February" ? "02"
                        : monthname == "March" ? "03"
                            : monthname == "April" ? "04"
                                : monthname == "May" ? "05"
                                    : monthname == "June" ? "06"
                                        : monthname == "July" ? "07"
                                            : monthname == "August" ? "08"
                                                : monthname == "September" ? "09"
                                                    : monthname == "October" ? "10"
                                                        : monthname == "November" ? "11"
                                                            : monthname == "December" ? "12" : null;
                var day = $('#datetimepicker3').val().split(" ")[1].split(",")[0];
                var hour = $('#ampmpicker').val() == "PM" && parseInt($('#hourpicker').val()) < 12 ? (parseInt($('#hourpicker').val()) + 12).toString() : $('#ampmpicker').val() == "AM" && $('#hourpicker').val() == "12"  ? "00" : $('#hourpicker').val();
                var meetingDateTime = $('#datetimepicker3').val().split(" ")[2] + month + day + "T" + hour + $('#minutepicker').val().toString() + "00";
                var now_newdate = new Date();
                var now_year = now_newdate.getFullYear().toString();
                var now_month = (now_newdate.getMonth() + 1).toString().length == 1 ? "0" + (now_newdate.getMonth() + 1).toString() : (now_newdate.getMonth() + 1).toString();
                var now_day = (now_newdate.getDate()).toString().length == 1 ? "0" + (now_newdate.getDate()).toString() : (now_newdate.getDate()).toString();
                var now_hour = now_newdate.getHours().toString().length == 1 ? "0" + now_newdate.getHours().toString() : now_newdate.getHours().toString();
                var now_minute = now_newdate.getMinutes().toString().length == 1 ? "0" + now_newdate.getMinutes().toString() : now_newdate.getMinutes().toString();
                var now_seconds = "00";
                var now = now_year + now_month + now_day + "T" + now_hour + now_minute + now_seconds;
                var year_from_now = (now_newdate.getFullYear() + 1).toString() + now_month + now_day + "T" + now_hour + now_minute + now_seconds;




                var isBlank = $('#datetimepicker3').val() == "" ? true : false;;
                if (isBlank) {
                    $('#meetingdate-required').show();
                    $('#datetimepicker3').css('border', '1px solid #dc3545');
                    $('#datetimepicker3').addClass('errorz');
                } else if (!isBlank) {
                    $('#datetimepicker').css('border', '1px solid green');
                    $('#meetingdate-required').hide();
                    $('#meetingdate-after-now').hide();
                    $('#meetingdate-in-one-year').hide();
                    $('#datetimepicker3').removeClass('errorz');
                    if (now <= meetingDateTime && meetingDateTime <= year_from_now) {
                        $('#datetimepicker3').css('border', '1px solid green');
                        $('#datetimepicker3').removeClass('errorz');
                    } else if (meetingDateTime > year_from_now) {
                        $('#meetingdate-in-one-year').show();
                        $('#datetimepicker3').css('border', '1px solid #dc3545');
                        $('#datetimepicker3').addClass('errorz');
                    } else if (now > meetingDateTime) {
                        $('#datetimepicker3').css('border', '1px solid #dc3545');
                        $('#meetingdate-after-now').show();
                        $('#datetimepicker3').addClass('errorz');
                    }

                }
