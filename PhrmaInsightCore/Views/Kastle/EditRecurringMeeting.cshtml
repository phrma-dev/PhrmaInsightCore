<h2 style="text-align: center;">Update Meeting Details</h2>
<div class="container" style="margin-top: 25px; border: 1px solid #cdd4e0; padding: 20px; overflow: hidden;">

	<div class="" style="background-color: #afc3d461;
    width: 150%;
    position: relative;
    left: -20px;
    padding-left: 20px;
    font-weight: bolder;
    font-size: 20px;">
		Visit Details
	</div>
	<div class="row row-sm">
		<div class="col">
			<label style="font-size: 13px;    margin-bottom: 2px;    font-weight: 400;    color: #97a3b9;" class="form-label">Meeting Date<span class="tx-danger"> *</span></label>
			<div class="input-group">
				<div class="input-group-prepend">
					<div class="input-group-text">
						<i class="typcn typcn-calendar-outline tx-24 lh--9 op-6"></i>
					</div>
				</div>
				<input type="text" value="@ViewBag.NiceMeetingDate" id="datetimepicker3" class="form-control errorz">

			</div>
			<span id="meetingdate-required" style="color: red; font-size: 13px; display: none;">This is required</span>
			<span id="meetingdate-after-now" style="color: red; font-size: 13px; display: none;">Meeting cannot be in the past</span>
			<span id="meetingdate-in-one-year" style="color: red; font-size: 13px; display: none;">Meeting must be within 1 year</span>
		</div>
		<div class="col">
			<label style="font-size: 13px;    margin-bottom: 2px;    font-weight: 400;    color: #97a3b9;" class="form-label">Meeting Time<span class="tx-danger"> *</span></label>
			<div class="row" style="position: relative; left: 15px; top: -25px;">
				<div style="margin-right: 1px;">
					<div id="ampm-div" style="font-size: 18px; font-weight: bold; text-align: center; background-color: none; color: black;">
						<select id="hourpicker" style="border: none; position: relative; top: 30px; -webkit-appearance: none; border-bottom: 0.2px solid blue;">
							<option value="01">1</option>
							<option value="02">2</option>
							<option value="03">3</option>
							<option value="04">4</option>
							<option value="05">5</option>
							<option value="06">6</option>
							<option value="07">7</option>
							<option value="08">8</option>
							<option value="09">9</option>
							<option value="10">10</option>
							<option value="11">11</option>
							<option value="12">12</option>
						</select>
					</div>
				</div>
				<div style="position: relative; margin-right: 5px;">
					<select style="border: none; position: relative; top: 30px;  -webkit-appearance: none;  -webkit-appearance: none;">
						<option>:</option>
					</select>
				</div>
				<div style="position: relative; margin-right: 5px;">
					<div id="ampm-div" style="font-size: 18px; text-align: center; background-color: none; color: black;">
						<select id="minutepicker" style="border: none; position: relative; top: 30px; -webkit-appearance: none; border-bottom: 0.2px solid blue;">
							<option value="00">00</option>
							<option value="15">15</option>
							<option value="30">30</option>
							<option value="45">45</option>
						</select>
					</div>
				</div>
				<div style="position: relative;">
					<div style="font-size: 18px; font-weight: bold; text-align: center; background-color: none; color: black; margin-right: 5px;">
						<select id="ampmpicker" style="border: none; position: relative; top: 30px; -webkit-appearance: none;">
							<option value="AM">AM</option>
							<option value="PM">PM</option>
						</select>
					</div>
				</div>
			</div>
		</div>
	</div><!-- row -->
	<div class="row row-sm">
		<div class="col-12">
			<label class="form-label" style="font-size: 13px; margin-bottom: 2px; font-weight: 400; color: #97a3b9;">Subject<span class="tx-danger"> *</span></label>
