<script id="evalScript">


        $(document).ready(function () {
            $('#submitForm').click(function () {
                    var monthname = $('#datetimepicker3').val().split(" ")[0];
                    var month = monthname == "January" ? "01"
                    : monthname == "February" ? "02"
                    : monthname == "March" ? "03"
                    : monthname == "April" ? "04"
                    : monthname == "May" ? "05"
                    : monthname == "June" ? "06"
                    : monthname == "July" ? "07"
                    : monthname == "August" ? "08"
                    : monthname == "September" ? "09"
                    : monthname == "October" ? "10"
                    : monthname == "November" ? "11"
                    : monthname == "December" ? "12" : null;
                    var day = $('#datetimepicker3').val().split(" ")[1].split(",")[0];

                    var hour = $('#ampmpicker').val() == "PM" && parseInt($('#hourpicker').val()) < 12 ? (parseInt($('#hourpicker').val()) + 12).toString() : $('#ampmpicker').val() == "AM" && $('#hourpicker').val() == "12"  ? "00" : $('#hourpicker').val();

                    var meetingDateTime = $('#datetimepicker3').val().split(" ")[2] + month + day + "T" + hour + $('#minutepicker').val().toString() + "00";
                    var endTime = $('#datetimepicker3').val().split(" ")[2] + month + $('#datetimepicker3').val().split(" ")[1].split(",")[0] + "T235959";
                    var singleusers = $('#users').find('.singleuser.active');
                    $.each(singleusers, function (k) {
                    if (singleusers.eq(k).find('#firstname').val().toString() != "" && singleusers.eq(k).find('#lastname').val().toString() != "") {
                    setTimeout(function () {
                    var vEmail = singleusers.eq(k).find('#visitoremail').val().toString() != "" ? singleusers.eq(k).find('#visitoremail').val().toString() : "no-email@phrma.org";
                    $.ajax({
                        url: '../Kastle/NewRecurringMeeting',
                        contentType: 'application/json',
                        dataType: 'json',
                        type: 'POST',
                        data: JSON.stringify({
                            "GroupId": "@ViewBag.GroupId",
                            "MeetingDate": meetingDateTime,
                            "Subject": $('#subject').val().toString(),
                            "Host": $('#host').val().toString(),
                            "NotifyEmail": $('#notify').val().toString(),
                            "VisitorFirstName": singleusers.eq(k).find('#firstname').val().toString(),
                            "VisitorLastName": singleusers.eq(k).find('#lastname').val().toString(),
                            "VisitorEmail": vEmail,
                            "ImpexiumGroup": "None",
                            "ImpexiumGroupCode": "None",
                            "RecurringId": "@ViewBag.RecurringId",
                            "RecurringInterval": "@ViewBag.RecurringInterval",
                            "VisitorOrganization": singleusers.eq(k).find('#visitororganization').val().toString(),
                            "EndDate": endTime,
                            "NiceMeetingDate": $('#datetimepicker3').val().toString(),
                            "NiceMeetingTime": $('#hourpicker').val().toString() + ":" + $('#minutepicker').val().toString() + " " + $('#ampmpicker').val().toString()
                        })
                    });
                    }, 200);
                    }

                    });


                    $.ajax({
                    url: '../Kastle/UpdateRecurringMeeting',
                    contentType: 'application/json',
                    dataType: 'json',
                    type: 'POST',
                    data: JSON.stringify({
                    "GroupId": "@ViewBag.GroupId",
                    "MeetingDate": meetingDateTime,
                    "Subject": $('#subject').val().toString(),
                    "Host": $('#host').val().toString(),
                    "NotifyEmail": $('#notify').val().toString(),
                    "EndDate": endTime,
                    "NiceMeetingDate": $('#datetimepicker3').val().toString(),
                    "NiceMeetingTime": $('#hourpicker').val().toString() + ":" + $('#minutepicker').val().toString() + " " + $('#ampmpicker').val().toString()

                    })
                    });

                    Swal.fire({
                    title: 'Meeting Updated!',
                    type: 'success',
                    confirmButtonColor: '#3085d6',
                    cancelButtonColor: '#d33',
                    confirmButtonText: 'OK'

                    }).then((result) => {
                    if (result.value) {
                    setTimeout(function () {
                    window.location.href = "../Kastle/MyRecurringMeetings";
                    },1500);

                    }
                    });



            });

        });

</script>
<script>
	$(document).ready(function () {

		document.getElementById('file').onchange = function () {
			var file = this.files[0];

			var reader = new FileReader();
			reader.onload = function (progressEvent) {
				// Entire file
				console.log(this.result);

				// By lines
				var lines = this.result.split('\n');
				var data = [];
				for (var line = 1; line < lines.length; line++) {

					var dataItem = [];
					var tabs = lines[line].split('\t');
					if (tabs != "") {
						for (var tab = 0; tab < tabs.length; tab++) {
							for (var i = 0; i < tabs[tab].split('\t').length; i++) {
								dataItem.push(tabs[tab].split('\t')[i].split('\r')[0]);
							}
						}
						if (dataItem[0] != "") {
							data.push(dataItem);
						}
					}


				}

				var singleusers = $('#users').find('.singleuser');

				$.each(data, function (q) {
					singleusers.eq(q).addClass('active').show().find('#firstname').val(data[q][0]);
					singleusers.eq(q).find('#lastname').val(data[q][1]);
					singleusers.eq(q).find('#visitoremail').val(data[q][2]);
					singleusers.eq(q).find('#visitororganization').val(data[q][3]);

				});

			};
			reader.readAsText(file);
		};

	});
</script>
