                    $('#minutepicker').find('option[value=@ViewBag.Time.Split(":")[1].Split(" ")[0].ToString()]').attr('selected', 'selected');;
                    $('#ampmpicker').find('option[value=@ViewBag.Time.Split(":")[1].Split(" ")[1].ToString()]').attr('selected', 'selected');

                }
            });
            $('#host').on('select2:close', function (e) {
                var isBlank = $('#host').val() == "" ? true : false;;
                if (isBlank) {
                    $('#select2-host-container').css('border', '1px solid #dc3545');
                    $('#host-required').show();
                } else if (!isBlank) {
                    $('#select2-host-container').css('border', '1px solid green');
                    $('#host-required').hide();
                }
            });
            $('#subject').change(function (e) {
                var isBlank = $('#subject').val() == "" ? true : false;;
                if (isBlank) {
                    $('#subject').css('border', '1px solid #dc3545');
                    $('#subject-required').show();
                } else if (!isBlank) {
                    $('#subject').css('border', '1px solid green');
                    $('#subject-required').hide();
                }
            });
            $('#datetimepicker3, #hourpicker, #minutepicker, #ampmpicker').change(function () {
                if ($('#host').val() != "" && $('#subject').val() != "" && $('#datetimepicker3').hasClass('errorz') == false) {
                    $('#submitForm').show();
                } else {
                    $('#submitForm').hide()
                }
            });
            $('#host').on('select2:close', function () {
                if ($('#host').val() != "" && $('#subject').val() != "" && $('#datetimepicker3').hasClass('errorz') == false) {
                    $('#submitForm').show();
                } else {
                    $('#submitForm').hide()
                }
            });
            $('#subject').change(function () {
                if ($('#host').val() != "" && $('#subject').val() != "" && $('#datetimepicker3').hasClass('errorz') == false) {
                    $('#submitForm').show();
                } else {
                    $('#submitForm').hide()
                }
            });
            $('#add_user').click(function () {
                $('.singleuser.active').last().next().show().addClass('active');
            });

            $('.az-header-menu > .nav').hide();
        });
